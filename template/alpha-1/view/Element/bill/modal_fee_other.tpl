<div id="modal-fee-other" class="modal">
    <div class="modal-header">
        Thêm chi phí khác
        <i class="material-icons f-s-14 lh-36 right modal-close">close</i>
    </div>
    <div class="modal-content p-v-xs">
        <div id="list-fee-other">
            <div class="row no-m">
                <div class="input-field col w-65">
                    <input type="text" autocomplete="off" value="" class="name-fee-other" placeholder="Tên chi phí">
                </div>
                <div class="input-field col w-30">
                    <input type="text" autocomplete="off" value="" class="auto-numeric right-align fee-other" placeholder="Chi phí">
                </div>
                <div class="input-field col w-5 no-p center-align">
                    <i class="material-icons f-s-13 lh-45 cursor-p delete-fee-other">close</i>
                </div>
            </div>
        </div>

        <div class="row no-m p-v-xs">
            <div class="col w-65">
                <a id="btn-add-fee-other" class="waves-effect waves-light btn green">
                    <i class="material-icons left">add</i>
                    Thêm chi phí
                </a>
            </div>
            <div class="col w-30">
                Tổng: <span id="label-total-fee-other" class="right"></span>
            </div>            
        </div>
    </div>
    <div class="modal-footer p-h-xs">
        <a id="btn-save-fee-other" class="waves-effect waves-light btn s6">
            Đồng ý
        </a>
        <a class="modal-close waves-effect waves-light btn black m-r-xs s6">
            Đóng
        </a>        
    </div>   
</div>