<div id="modal-fee-other" class="modal">
    <div class="modal-header">
        Thêm nhà cung cấp
        <i class="material-icons f-s-14 lh-36 right modal-close">close</i>
    </div>
    <div class="modal-content p-v-xs">
       <div class="row no-m">            
            <div class="input-field col s12 m6 l6">
                <input id="name_supplier" type="text" autocomplete="off" value="">
                <label for="name_supplier">
                    Tên nhà cung cấp
                    <small class="label-required text-red">*</small>
                </label>
            </div>

            <div class="input-field col s12 m6 l6">
                <input id="email_supplier" type="text" autocomplete="off" value="">
                <label for="email_supplier">
                    Email
                </label>
            </div>

            <div class="input-field col s12 m6 l6">
                <input id="phone_supplier" type="text" autocomplete="off" value="">
                <label for="phone_supplier">
                    Số điện thoại
                </label>
            </div>

            <div class="input-field col s12 m6 l6">
                <input id="group_supplier_suggest" type="text" autocomplete="off" value="">
                <label for="group_supplier_suggest">
                    Nhóm nhà cung cấp
                </label>
                <input id="group_supplier" type="hidden" value="" />
            </div>

            <div class="input-field col s12 m6 l6">
                <input id="address_supplier" type="text" autocomplete="off" value="">
                <label for="address_supplier">
                    Địa chỉ
                    <small class="label-required text-red">*</small>
                </label>
            </div>

            <div class="input-field col s12 m6 l6">
                <input id="area_supplier" type="text" autocomplete="off" value="">
                <label for="area_supplier">
                    Khu vực
                </label>

                <input id="city_id_supplier" type="hidden" value="" />
                <input id="district_id_supplier" type="hidden" value="" />
            </div>
       </div>        
    </div>
    <div class="modal-footer p-h-xs">
        <a id="btn-add-supplier" class="waves-effect waves-light btn s6">
            Đồng ý
        </a>
        <a class="modal-close waves-effect waves-light btn black m-r-xs s6">
            Đóng
        </a>        
    </div>   
</div>  